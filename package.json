{
  "name": "@incutonez/jefharkay",
  "private": true,
  "version": "2.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "explode": "npm cache clean --force && npx rimraf package-lock.json **/node_modules --glob && npm i",
    "preview": "vite preview",
    "lint": "npx eslint --fix",
    "update:deps": "node ./updateDependencies.js",
    "update:versions": "node ./updateVersions.js",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "~3.5.27",
    "vue-router": "~4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "~9.39.2",
    "@incutonez/eslint-plugin": "~2.1.4",
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/exec": "~7.1.0",
    "@semantic-release/git": "~10.0.1",
    "@stylistic/eslint-plugin-ts": "~4.4.1",
    "@tailwindcss/vite": "~4.1.18",
    "@types/node": "~25.0.10",
    "@typescript-eslint/eslint-plugin": "~8.53.1",
    "@typescript-eslint/parser": "~8.53.1",
    "@vitejs/plugin-vue": "~6.0.3",
    "@vue/tsconfig": "~0.8.1",
    "conventional-changelog-conventionalcommits": "~9.1.0",
    "eslint": "~9.39.2",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "eslint-plugin-vue": "~10.7.0",
    "globals": "~17.1.0",
    "husky": "~9.1.7",
    "lint-staged": "~16.2.7",
    "semantic-release": "~25.0.2",
    "tailwindcss": "~4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.53.1",
    "vite": "~7.3.1",
    "vue-tsc": "~3.2.4"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
      "npm run lint"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
